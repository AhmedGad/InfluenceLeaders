create table userid(
    id BigInt,
    PRIMARY KEY ( id )
);

create table graph(
    id1 BigInt,
    id2 BIgInt,
    
    FOREIGN KEY ( id1 ) REFERENCES userid ( id ),
    FOREIGN KEY ( id2 ) REFERENCES userid ( id )
);

create table queue(
    id BigInt,
    FOREIGN KEY ( id ) REFERENCES userid ( id )
);


delete from userid;
delete from queue;
delete from graph;

insert into userid value(280089745);
insert into queue value(280089745);

